{
  "name": "nest-with-typescript-and-typeorm",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "https://github.com/iago-f-s-e/template-nest-with-typescript-and-typeorm.git",
  "author": "iago-f-s-e <iago.fagundes@outlook.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": " eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typecheck": "yarn tsc --noEmit",
    "prepare:husky": "husky install",
    "make:env": "chmod +x ./infra/make-env.sh && bash make.sh",
    "make:seeds": "chmod +x ./infra/make-migration.sh && bash ./infra/make-seeds.sh",
    "make:migration": "chmod +x ./infra/make-migration.sh && bash ./infra/make-migration.sh",
    "migration:gen": "yarn typeorm migration:generate -p -d src/infra/database/cli/default-connection.ts",
    "migration:run": "yarn typeorm migration:run -d src/infra/database/cli/default-connection.ts",
    "migration:rev": "yarn typeorm migration:revert -d src/infra/database/cli/default-connection.ts",
    "seeds:gen": "yarn typeorm migration:generate -d src/infra/database/cli/seeds-connection.ts",
    "seeds:run": "yarn typeorm migration:run -d src/infra/database/cli/seeds-connection.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:run": "yarn typeorm:migration:run && yarn typeorm:seeds:run"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/typeorm": "^8.0.4",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "helmet": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
